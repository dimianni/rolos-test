@import 'sanitize.css';

html {
  height: 100%;
}

body {
  height: 100%;
  color: #333;
  font-weight: 400;
  font-family: sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  background-color: #f0f0f0;
}

#app {
  display: flex;
  flex-direction: column;
  min-height: 100%;
}

a {
  color: currentColor;
  text-decoration: none;
  cursor: pointer;
}
